@startuml
Customer *- "*" Rental
Rental o- Movie

class Customer {
    - String _name
    - Vector<Rental> _rentals
    + String statement ()
    }

class Rental {
    - int _daysRented
    - Movie _movie
    + int getDaysRented ()
    + Movie getMovie ()
    }

class Movie {
    - int _priceCode
    - String _title
    + getPriceCode ()
    + getTitle ()
    }

@enduml
